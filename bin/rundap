#!/usr/bin/env python

from __future__ import print_function

validation=True
try: 
    from mangadap import rundap
except ImportError as e: 
    validation = False
    print('ImportError: {0}'.format(str(e)))
if validation:
    rundap = rundap(console=True)

"""example usage:
    rundap --daily
    rundap --all
    rundap --all --clobber
    rundap --redo
"""
